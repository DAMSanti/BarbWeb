{
  "name": "bufete-backend",
  "version": "1.0.1",
  "description": "Backend for law firm consultation platform with OpenAI integration",
  "type": "module",
  "main": "dist/index.js",
  "scripts": {
    "dev": "node --loader ts-node/esm src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "preview": "node dist/index.js",
    "db:push": "prisma db push",
    "db:seed": "node --loader ts-node/esm prisma/seed.ts",
    "db:generate": "prisma generate",
    "db:migrate": "prisma migrate dev"
  },
  "keywords": ["law", "consulting", "openai", "express"],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "@google/generative-ai": "^0.1.3",
    "axios": "^1.6.2",
    "typescript": "^5.3.3",
    "@types/express": "^4.17.21",
    "@types/node": "^20.10.4",
    "@types/cors": "^2.8.17",
    "@prisma/client": "^5.7.0"
  },
  "devDependencies": {
    "ts-node": "^10.9.2",
    "prisma": "^5.7.0"
  }
}
