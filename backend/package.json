{
  "name": "bufete-backend",
  "version": "1.0.1",
  "description": "Backend for law firm consultation platform with OpenAI integration",
  "type": "module",
  "main": "dist/index.js",
  "scripts": {
    "dev": "node --loader ts-node/esm src/index.ts",
    "build": "prisma generate && tsc",
    "start": "npm run db:push:silent && node dist/index.js",
    "preview": "node dist/index.js",
    "db:push": "prisma db push --skip-generate --accept-data-loss",
    "db:push:silent": "prisma db push --skip-generate --accept-data-loss 2>/dev/null || true",
    "db:seed": "node --loader ts-node/esm prisma/seed.ts",
    "db:generate": "prisma generate",
    "db:migrate": "prisma migrate dev"
  },
  "keywords": ["law", "consulting", "openai", "express"],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "@google/generative-ai": "^0.1.3",
    "axios": "^1.6.2",
    "typescript": "^5.3.3",
    "@types/express": "^4.17.21",
    "@types/node": "^20.10.4",
    "@types/cors": "^2.8.17",
    "@prisma/client": "^5.7.0",
    "jsonwebtoken": "^9.0.2",
    "bcryptjs": "^2.4.3",
    "passport": "^0.7.0",
    "passport-google-oauth20": "^2.0.0"
  },
  "devDependencies": {
    "ts-node": "^10.9.2",
    "prisma": "^5.7.0",
    "@types/jsonwebtoken": "^9.0.7",
    "@types/bcryptjs": "^2.4.6",
    "@types/passport": "^1.0.16",
    "@types/passport-google-oauth20": "^2.0.14"
  }
}
