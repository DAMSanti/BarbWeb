{
  "name": "bufete-backend",
  "version": "1.0.1",
  "description": "Backend for law firm consultation platform with OpenAI integration",
  "type": "module",
  "main": "dist/index.js",
  "engines": {
    "node": "20.x",
    "npm": "10.x"
  },
  "scripts": {
      "revoke:refresh-tokens": "node ./scripts/revoke-refresh-tokens.js",
      "check-env": "node ./scripts/check-env.js",
      "validate:domains": "node ./scripts/validate-domains.js",
      "validate:secrets": "node ./scripts/validate-secrets.js",
      "generate:secrets": "node ./generate-secrets.js",
      "generate:secrets:value": "node ./generate-secrets.js --value",
      "generate:secrets:json": "node ./generate-secrets.js --json",
    "dev": "node --loader ts-node/esm src/index.ts",
    "build": "tsc",
    "start": "sh start.sh",
    "preview": "node dist/index.js",
    "test": "npx vitest run",
    "test:watch": "npx vitest",
    "test:coverage": "npx vitest run --coverage",
    "test:e2e": "npx playwright test",
    "db:init": "node --loader ts-node/esm prisma/init-db.ts || echo 'DB already initialized'",
    "db:push": "prisma db push --skip-generate --accept-data-loss",
    "db:seed": "node --loader ts-node/esm prisma/seed.ts",
    "db:generate": "prisma generate",
    "db:migrate": "prisma migrate dev"
  },
  "keywords": [
    "law",
    "consulting",
    "openai",
    "express"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@google/generative-ai": "^0.1.3",
    "@prisma/client": "^5.7.0",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "axios": "^1.6.2",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "passport": "^0.7.0",
    "passport-google-oauth20": "^2.0.0",
    "resend": "^6.4.2",
    "stripe": "^19.3.0",
    "winston": "^3.18.3",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@playwright/test": "^1.56.1",
    "@types/bcryptjs": "^2.4.6",
    "@types/jsonwebtoken": "^9.0.7",
    "@types/node": "^24.10.1",
    "@types/passport": "^1.0.16",
    "@types/passport-google-oauth20": "^2.0.14",
    "@vitest/coverage-v8": "^2.0.0",
    "bcrypt": "^6.0.0",
    "prisma": "^5.7.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3",
    "vitest": "^2.0.0"
  }
}
